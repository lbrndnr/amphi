<.header>
  New Post
  <:subtitle>Use this form to manage post records in your database.</:subtitle>
</.header>

<.simple_form :let={f} for={@changeset} action={~p"/posts"}>
    <.input field={f[:url]} type="url" label="URL" />
    <:actions>
        <.button>Save</.button>
    </:actions>
</.simple_form>

<.back navigate={~p"/"}>Back to Feed</.back>
